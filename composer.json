{
    "name": "genesis-advanced",
    "type": "wordpress-theme",
    "license": "GPL-2.0-or-later",
    "description": "Starter theme for Genesis Framework with a modern development workflow",
    "homepage": "https://thematicpress.com/genesis-advanced/",
    "authors": [
        {
            "name": "Nic Beltramelli",
            "email": "nicbeltramelli@gmail.com",
            "homepage": "https://github.com/NicBeltramelli/"
        }
    ],
    "keywords": [
    "wordpress",
    "genesis"
    ],
    "require": {
        "php": ">=7.1",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.2",
        "wp-coding-standards/wpcs": "^0.14.1",
        "wimg/php-compatibility": "^8.1",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4"
    },
    "scripts": {
        "phpcs": "phpcs",
        "export": "zip -r ../genesis-advanced.zip * .[^.]* -x '*.log' -x '.git/*' -x '.cache-loader/*' -x 'node_modules/*' -x 'vendor/*'"
    }
}
