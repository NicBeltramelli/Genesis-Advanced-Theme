{
    "name": "nicbeltramelli/space",
    "type": "wordpress-theme",
    "license": "GPL-2.0-or-later",
    "description": "Starter theme for Genesis Framework with a modern development workflow",
    "homepage": "https://thematicpress.com/space/",
    "authors": [
        {
            "name": "Nic Beltramelli",
            "email": "nicbeltramelli@gmail.com",
            "homepage": "https://github.com/NicBeltramelli/"
        }
    ],
    "keywords": [
    "wordpress",
    "genesis"
    ],
    "require": {
        "php": ">=7.1",
        "composer/installers": "~1.0",
        "wpackio/enqueue": "^2.1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.2",
        "wp-coding-standards/wpcs": "^0.14.1",
        "phpcompatibility/php-compatibility": "^9.1.1",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4"
    },
    "scripts": {
        "phpcs": "phpcs",
        "phpcbf": "phpcbf --report=diff -vv",
        "export": "zip -r ../space.zip * .[^.]* -x '.*' '.git/*' '*.log' '*.lock' '.cache-loader/*' 'phpcs.xml.dist' 'composer.json' 'package.json' 'assets/*' 'node_modules/*' 'vendor/*'"
    }
}
